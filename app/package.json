{
  "name": "@staticdeploy/app",
  "description": "staticdeploy app",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "author": "Paolo Scanferla <paolo.scanferla@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/staticdeploy/staticdeploy.git"
  },
  "bugs": {
    "url": "https://github.com/staticdeploy/staticdeploy/issues"
  },
  "scripts": {
    "prettier": "prettier '@(src|test)/**/*.@(ts|tsx|css)'",
    "prettify": "yarn prettier --write",
    "lint:prettier": "yarn prettier --list-different",
    "lint:tslint": "tslint '@(src|test)/**/*.ts?(x)'",
    "lint": "npm-run-all lint:*",
    "test": "TS_NODE_TYPE_CHECK=true TS_NODE_PROJECT=test NODE_ENV=test mocha --exit -r test/setup --watch-extensions ts,tsx 'test/**/*.ts?(x)'",
    "dev:mock-server": "TS_NODE_TYPE_CHECK=true TS_NODE_PROJECT=mock-server mock-server --delay 1000 --watch --require ts-node/register --serveConfig",
    "dev:server": "webpack-dev-server --config webpack/dev.js",
    "dev": "npm-run-all -p dev:*",
    "build": "webpack --config webpack/prod.js"
  },
  "dependencies": {
    "antd": "^3.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@staticdeploy/app-server": "^1.1.1",
    "@staticdeploy/mock-server": "^1.6.2",
    "@types/chai": "^4.0.5",
    "@types/enzyme": "^3.1.4",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/express": "^4.0.39",
    "@types/faker": "^4.1.2",
    "@types/lodash": "^4.14.79",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.5.5",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.2",
    "@types/react-router-dom": "^4.0.8",
    "@types/sinon": "^4.0.0",
    "@types/sinon-chai": "^2.7.29",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^2.30.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.4",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.7.4",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "ts-loader": "^3.1.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-react": "^3.3.3",
    "typescript": "^2.6.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  }
}
