{
  "name": "@staticdeploy/api-server",
  "description": "staticdeploy api server",
  "version": "0.11.1",
  "private": true,
  "main": "build/server.js",
  "license": "MIT",
  "author": "Paolo Scanferla <paolo.scanferla@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/staticdeploy/staticdeploy.git"
  },
  "bugs": {
    "url": "https://github.com/staticdeploy/staticdeploy/issues"
  },
  "scripts": {
    "compile": "rimraf build && tsc",
    "prettier": "prettier 'src/**/*.ts' 'test/**/*.ts'",
    "prettify": "yarn prettier --write",
    "lint:prettier": "yarn prettier --list-different",
    "lint:tslint": "tslint 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "yarn lint:prettier && yarn lint:tslint",
    "test": "NODE_ENV=test NODE_PATH=src mocha --exit -t 20000 -r ts-node/register --watch-extensions ts 'test/**/*.ts'",
    "start": "NODE_ENV=production NODE_PATH=build node build/server.js",
    "docker:build": "docker build -f ./Dockerfile -t staticdeploy/api-server:$DOCKER_TAG ../",
    "docker:push": "docker push staticdeploy/api-server:$DOCKER_TAG"
  },
  "dependencies": {
    "@mondora/env": "^1.3.0",
    "@staticdeploy/common-types": "^0.11.1",
    "@staticdeploy/storage": "^0.11.1",
    "bunyan": "^1.8.12",
    "bunyan-middleware": "^0.8.0",
    "convexpress": "^2.3.0",
    "express": "^4.17.1",
    "express-healthchecker": "^1.0.0",
    "express-jwt": "^5.3.1"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.1.7",
    "@types/chai-fuzzy": "^1.3.30",
    "@types/express": "^4.17.0",
    "@types/express-jwt": "^0.0.42",
    "@types/fs-extra": "^8.0.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.136",
    "@types/mocha": "^5.2.7",
    "@types/s3rver": "^0.0.31",
    "@types/sinon": "^7.0.13",
    "@types/supertest": "^2.0.8",
    "@types/tar": "^4.0.3",
    "@types/uuid": "^3.4.5",
    "chai": "^4.2.0",
    "chai-fuzzy": "^1.6.1",
    "create-fs-tree": "^1.0.0",
    "fs-extra": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.14",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "s3rver": "^3.2.0",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2",
    "tar": "^4.4.10",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3",
    "uuid": "^3.3.2"
  }
}
